{
  "filters": {},
  "rules": {
    "no-start-duplicated-conjunction": true,
    "terminology": {
      "exclude": [
        "HTML",
        "ID",
        "URL"
      ]
    },
    "write-good": true,
    "diacritics": true,
    "doubled-spaces": true,
    "stop-words": {
      "exclude": [
        "so as to"
      ]
    },
    "@textlint-rule/no-unmatched-pair": true,
    "@textlint-rule/no-duplicate-abbr": true,
    "common-misspellings": {
      "ignore": [
        "CLAs"
      ] 
    },
  }
}
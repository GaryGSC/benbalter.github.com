{
  "ci": {
    "collect": {
      "staticDistDir": "./_site",
      "url": [
        "http://localhost/index.html",
        "http://localhost/about/index.html",
        "http://localhost/2021/09/01/how-i-re-over-engineered-my-home-network/index.html"
      ]
    }, 
    "assert": {
      "preset": "lighthouse:no-pwa",
      "assertions": {
        "csp-xss": "off",
        "unused-javascript": "off",
        "unused-css-rules": "off",
        "canonical": "warn"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
